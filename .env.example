# Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Google Cloud API Key for Speech-to-Text
# Get from: https://console.cloud.google.com/apis/credentials
GOOGLE_APPLICATION_CREDENTIALS=server/gcp_key.json

# Gemini API Key for AI features
# Get from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Server Port (optional - defaults to 4000)
PORT=4000

# Flask Environment (production/development)
FLASK_ENV=production

# Modal Endpoint (if using custom AI endpoint)
MODAL_ENDPOINT=https://your-modal-endpoint.modal.run
